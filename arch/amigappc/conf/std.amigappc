# standard amiga information

machine amigappc powerpc
include		"conf/std"	# MI standard options

mainbus0	at root

clock0	at mainbus0
kbd0	at mainbus0
zbus0	at mainbus0

options 	EXEC_ELF32
options 	EXEC_SCRIPT
options 	PPC_OEA
makeoptions	PPCDIR="oea"	# Tell Makefile.powerpc what dir to use
